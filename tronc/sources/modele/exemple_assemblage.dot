/*
  Diagramme d'un exemple d'assemblage.
*/

graph ExempleObjets {
	compound=true;
    node [shape=record, style=rounded];

  cockpit;
  coque;
  aile1;
  aile2;
  moteur1;
  moteur2;

	cockpit -- coque [label="fixe"] ;
	coque -- aile1 [label="fixe"] ;
	coque -- aile2 [label="fixe"] ;
	
	aile1 -- moteur1 [label="fixe"] ;
	aile2 -- moteur2 [label="fixe"] ;
}
