[Kernel]
log=false

[Input]

// mapping between joystick axis number and in-game axis
// here configuration for sidewinder forcefeedback 2
XAxis=0
YAxis=1
ZAxis=5
ThrottelAxis=6

[Model]

// ship guidance system property
GuidanceForce=5

// stabilizers properties
StabilizerForce=10

// engine property (try to keep value around 1)
EngineMaxForce=500

// dragger property (the dragger generate a force against linear movement)
DraggerCoeeficient=0.01

ShipMass=1

// initial speed of laser beam
LaserBeamSpeed=300

log=false

[Physic]

// Some physic parameters. Handle with care
WorldCFM=0.01
WorldERP=1

ActivateCollision=true

WorldContactSurfaceLayer=0.01
WorldContactMaxCorrectingVelocity=100
MaxNumberOfContactPoints=10

// contact parameters
ContactMu=0
// coefficient of restitution : see http://en.wikipedia.org/wiki/Coefficient_of_restitution 
ContactBounce=0.1
ContactBounceVelocity=5
ContactSoftErrorReduction=1
ContactSoftConstraintForceMixing=1

log=false

[AI]
log=false

[Agent]
log=false

[TestDisplay]
log=true

