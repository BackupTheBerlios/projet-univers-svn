[Demonstration]
log=false
[Input]
// wheither we should print the axis values when moved
PrintAxes=false

// mapping between joystick axis number and in-game axis
// here configuration for sidewinder forcefeedback 2
XAxis=0
YAxis=1
ZAxis=5
ThrottelAxis=6
log=true

[Model]
log=true
// ship guidance system property
GuidanceForce=5

// stabilizers properties
StabilizerForce=10

// engine property (try to keep value around 1)
EngineMaxForce=500

// dragger property (the dragger generate a force against linear movement)
DraggerCoeeficient=0.01

// initial speed of laser beam
LaserBeamSpeed=300


[Physic]

// Some physic parameters. Handle with care
WorldCFM=0.001
WorldERP=1

ActivateCollision=true

// some kind of ...
WorldContactSurfaceLayer=0
WorldContactMaxCorrectingVelocity=5
MaxNumberOfContactPoints=50

// contact parameters
ContactMu=0
ContactBounce=0
ContactBounceVelocity=0
ContactSofERP=1
ContactSofCFM=1

[Display]
log=false

// name of the Ogre material to use for target selection
ReticuleMaterial="PU/material/reticule"
// name of the Ogre material to use for target indication out of screen
ArrowMaterial="PU/material/arrow"
ArrowSize=0.1
[Kernel]
log=false


