	<!--
		***************************************************
		ProjetUnivers/TabbedWindow :  A container for tabs panels
			ProjetUnivers/TabButtonPane : The section that contains the buttons of the 
																	  tabs
																		Allow a background for that part
				ProjetUnivers/TabButton : A button in a multi-tab panel
		  ProjetUnivers/TabContentPane : a panel in a TabControl
		***************************************************
	-->
	<WidgetLook name="ProjetUnivers/TabbedWindow">
		<Property name="TabButtonType" value="ProjetUnivers/TabButton" />
		<Property name="TabTextPadding" value="{0,6}" />
		<!-- 
			Contains the buttons for the tabs  
		-->
		<Child type="ProjetUnivers/TabButtonPane"
			nameSuffix="__auto_TabPane__Buttons">
			<Area>
				<Dim type="LeftEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="TopEdge">
					<AbsoluteDim value="0" />
				</Dim>
				<Dim type="Width">
					<UnifiedDim scale="1" type="Width" />
				</Dim>
				<Dim type="Height">
					<PropertyDim name="TabHeight" type="Height" />
				</Dim>
			</Area>
		</Child>
		<!-- 
			The panel container, take the bottom rest of the window :
					0	      ................
                  |              |
      TabHeight   ................
          ^       |              |
          |       |              |
          |       |              |
      1-TabHeight ................
    -->
    <Child type="ProjetUnivers/TabContentPane"
      nameSuffix="__auto_TabPane__">
      <Area>
        <Dim type="LeftEdge">
          <AbsoluteDim value="0" />
        </Dim>
        <Dim type="TopEdge">
          <PropertyDim name="TabHeight" type="Height" />
        </Dim>
        <Dim type="Width">
          <UnifiedDim scale="1" type="Width" />
        </Dim>
        <Dim type="Height">
          <UnifiedDim scale="1" type="Height">
            <DimOperator op="Subtract">
              <PropertyDim name="TabHeight" type="Height" />
            </DimOperator>
          </UnifiedDim>
        </Dim>
      </Area>
    </Child>
    <StateImagery name="Enabled" />
    <StateImagery name="Disabled" />
  </WidgetLook>

